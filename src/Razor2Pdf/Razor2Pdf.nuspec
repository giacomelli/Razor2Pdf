<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Razor2Pdf</id> 
    <version>1.0.0</version>
    <authors>Diego Giacomelli</authors>
    <owners>Diego Giacomelli</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://github.com/giacomelli/Razor2Pdf/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/giacomelli/Razor2Pdf</projectUrl>
    <description>Razor2Pdf is a easy-to-use, cross-platform library to build PDF files from Razor template files.</description>
    <releaseNotes>See changes at https://github.com/giacomelli/Razor2Pdf.</releaseNotes>
    <tags>razor pdf</tags>
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="DinkToPdf" version="1.0.8" exclude="Build,Analyzers" />
        <dependency id="System.Runtime.Loader" version="4.3.0" exclude="Build,Analyzers" />
        <dependency id="razorlight" version="2.0.0-beta1" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <contentFiles>
      <files include="libwkhtmltox.dll" buildAction="Content" copyToOutput="true"/>
      <files include="libwkhtmltox.dylib" buildAction="Content" copyToOutput="true" />
      <files include="libwkhtmltox.readme" buildAction="Content" copyToOutput="true" />
      <files include="libwkhtmltox.so" buildAction="Content" copyToOutput="true" />
    </contentFiles>
  </metadata>
</package>